<script>
jQuery(document).ready(function() {     
Metronic.init(); // init metronic core components
Layout.init(); // init current layout
Login.init();
});

toastr.options = {
  "closeButton": true,
  "debug": false,
  "positionClass": "toast-top-full-width",
  "onclick": null,
  "showDuration": "1000",
  "hideDuration": "1000",
  "timeOut": "5000",
  "extendedTimeOut": "1000",
  "showEasing": "swing",
  "hideEasing": "linear",
  "showMethod": "fadeIn",
  "hideMethod": "fadeOut"
}

</script>
{% for type, messages in app.session.flashBag.all %}
{% for message in messages %}
{% if message %}
<script>
toastr['{{ type }}']("{{ message|trans({}, 'FOSUserBundle') }}", "Message")
</script>
{% endif %}
{% endfor %}
{% endfor %}

{% if error is defined and not error is empty  %}
<script>
toastr['error']("{{ error|trans({}, 'FOSUserBundle') }}", "Message");
</script>

{% endif %}

{% if form is defined and not form.vars.valid %}
<script>
toastr['error']("{{ form_errors(form) }}", "Formulaire invalid")
</script>
{% endif %}

{% if invalid_username is defined %}
<script>
toastr['error']("{{ 'resetting.request.invalid_username'|trans({'%username%': invalid_username}, 'FOSUserBundle') }}", "Invalide")
</script>
{% endif %}

{% endblock %}
