<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="UserBundle\Entity\User" table="fos_user">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="firstName" type="string" column="first_name" precision="0" scale="0" nullable="true"/>
    <field name="lastName" type="string" column="last_name" precision="0" scale="0" nullable="true"/>
    <field name="function" type="string" column="function" precision="0" scale="0" nullable="true"/>
    <field name="avatar" type="string" column="avatar" precision="0" scale="0" nullable="true"/>
    <field name="phone" type="string" column="phone" precision="0" scale="0" nullable="true"/>
    <field name="mobile" type="string" column="mobile" precision="0" scale="0" nullable="true"/>
    <field name="fb" type="string" column="fb" precision="0" scale="0" nullable="true"/>
    <field name="skype" type="string" column="skype" precision="0" scale="0" nullable="true"/>
  </entity>
</doctrine-mapping>
