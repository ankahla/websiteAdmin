<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="websites\ManagementBundle\Entity\FtpAccounts" table="ftp_accounts" repository-class="websites\ManagementBundle\Entity\Repository\FtpAccountsRepository">
    <indexes>
      <index name="id_website" columns="id_website"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idWebsite" type="integer" column="id_website" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="host" type="string" column="host" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="login" type="string" column="login" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="passwd" type="string" column="passwd" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
