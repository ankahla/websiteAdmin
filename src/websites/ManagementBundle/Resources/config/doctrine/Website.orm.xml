<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="websites\ManagementBundle\Entity\Website" table="website" repository-class="websites\ManagementBundle\Entity\Repository\WebsiteRepository">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="domain" type="string" column="domain" length="150" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="online" type="boolean" column="online" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default">1</option>
      </options>
    </field>
    <field name="ip" type="string" column="ip" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cpanel" type="string" column="cpanel" length="200" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cplogin" type="string" column="cplogin" length="200" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cppass" type="string" column="cppass" length="200" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="pop3" type="string" column="pop3" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="smtp" type="string" column="smtp" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="webmail" type="string" column="webmail" length="200" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dns1" type="string" column="dns1" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dns2" type="string" column="dns2" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="billing" type="string" column="billing" length="200" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="billinglogin" type="string" column="billinglogin" length="200" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="billingpass" type="string" column="billingpass" length="200" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="notes" type="text" column="notes" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="owner" type="string" column="owner" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="updDate" type="datetime" column="upd_date" nullable="false"/>
    <field name="createdDate" type="datetime" column="created_date" nullable="false"/>
  </entity>
</doctrine-mapping>
